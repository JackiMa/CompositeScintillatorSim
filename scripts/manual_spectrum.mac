/control/verbose 0
/tracking/verbose 0
/run/verbose 0
/control/cout/ignoreThreadsExcept 0
/run/initialize

# 设置粒子发射器为GPS
/CompScintSim/generator/useParticleGun false

# 设置粒子源
/gps/verbose 0
/gps/source/multiplevertex true

# 清除所有源
/gps/source/clear

# 添加源，每个源的粒子数，能量，放出该粒子的数量
/gps/my_source/add e- 5 10
/gps/my_source/add proton 10 5
/gps/my_source/add gamma 1 20



# 运行命令
/run/beamOn 1
